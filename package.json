    "test": "jest --detectOpenHandles"
  },
  "devDependencies": {
    "@swc/core": "^1.3.38",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.3",
    "jest": "^29.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^2.1.0"
  }
}
